<div class="spacemaker" style="margin-top:100px"></div>

<div class="container">
  <div class="form-container">
    <h2>Log in</h2>

    <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-inputs">
        <%= f.input :email, required: false, autofocus: true %>
        <%= f.input :password, required: false %>
        <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
      </div>

  <div class="spacemaker" style="margin-top:35px"></div>

    <div class="form-button1">
        <div class="form-actions">
          <%= f.button :submit, "Log in", class: "jd-solid-button-med"  %>
        </div>
      <% end %>
    </div>

  <div class="spacemaker" style="margin-top:15px"></div>

    <div class="form-button2">
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>

<div class="spacemaker" style="margin-top:100px"></div>

